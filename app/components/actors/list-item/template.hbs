<div class="page-header">
  <h1>Actors</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>#</th>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
  </thead>
  <tbody>
      {{#each actors as |actor|}}
        <tr>
          <th scope="row">{{actor.id}}</th>
          <td>{{actor.firstName}}</td>
          <td>{{actor.lastName}}</td>
        </tr>
      {{/each}}
  </tbody>
</table>
